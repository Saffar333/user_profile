{
  "version": 2,
  "rewrites": [
    {
      "source": "/profile",
      "destination": "/user_profile.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
